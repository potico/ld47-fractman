pico-8 cartridge // http://www.pico-8.com
version 29
__lua__

cls()

function draw(x0,y0,w)
x0*=1.5 x0-=40
y0*=1.5 y0-=40
w*=1.5
  -- draw background
  for j=0,15 do
    for i=0,15 do
      if mget(i,j)>2 then
        if i<8 or j<8 then
          local size=w*(1-min(i,j)/16)
          local x,y = x0+i*w,y0+j*w
--size=w
          rectfill(x-size*.75,y-size*.75,x+size*.75+1,y+size*.75+1,12)
          --rect(x-size*.75,y-size*.75,x+size*.75+1,y+size*.75+1,10)
        end
      end
    end
  end
  -- draw empty space
  for j=0,15 do
    for i=0,15 do
      if i<8 or j<8 then
        if mget(i,j)==2 then
          local size=w*(1-min(i,j)/16)
          local x,y = x0+i*w,y0+j*w
          rectfill(x-size*.75,y-size*.75,x+size*.75,y+size*.75,0)
          --rect(x-size*.75,y-size*.75,x+size*.75,y+size*.75,3)
        end
      end
    end
  end
  -- draw pellets
  for j=0,15 do
    for i=0,15 do
      if i<8 or j<8 then
        if mget(i,j)==2 then
          local size=w*(1-min(i,j)/16)
          local x,y = x0+i*w,y0+j*w
          --circfill(x,y,size/6,7)
          rectfill(x-size/8,y-size/8,x+size/8,y+size/8,7)
        end
      end
    end
  end
end

draw(0,0,8)
draw(64,64,4)
draw(96,96,2)
draw(112,112,1)
spr(52,47,28)

pacman =
{
  x=1
}

